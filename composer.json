{
    "name": "bluprint/bluprint",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "David Bonner",
            "email": "david@continuuminteractive.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.5",
        "league/commonmark": "^0.15.6",
        "jenssegers/optimus": "^0.2.3"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "friendsofphp/php-cs-fixer": "^2.4",
        "barryvdh/laravel-debugbar": "^3.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Bluprint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bluprint\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DavidIanBonner\\Presenter\\PresenterServiceProvider"
            ]
        }
    }
}
